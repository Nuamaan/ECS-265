r, MyFooter, Contracts }\n  // created: function() {\n  //   axios.get('/static/products.json')\n  //   .then((response) =>{\n  //     this.products=response.data.products;\n  //     console.log(this.products);\n  //   });\n  // }\n}\n</script>\n\n\n<style scoped>\n\ntextarea{\n  background-color:#d7dce2;\n  border: #7d7e7e solid;\n  border-width: 2px;\n  margin-bottom: 10px;\n}\n\np{\n  padding-left: 395px;\n}\n\nimg\n{\n  background: none;\n}\n\nh2, h3\n{\n  padding-top: 10px;\n}\n\n\n</style>"]}]}